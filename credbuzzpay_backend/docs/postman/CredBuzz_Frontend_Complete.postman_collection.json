        {
          "name": "Upload Selfie",
          "request": {
            "method": "POST",
            "body": {"mode": "formdata", "formdata": [{"key": "selfie", "type": "file", "src": ""}]},
            "url": "{{base_url}}/kyc/verification/selfie/"
          }
        },
        {
          "name": "Upload Office Photos",
          "request": {
            "method": "POST",
            "body": {"mode": "formdata", "formdata": [{"key": "inside_photo", "type": "file", "src": ""}, {"key": "outside_photo", "type": "file", "src": ""}]},
            "url": "{{base_url}}/kyc/verification/office/"
          }
        },
        {
          "name": "Upload Address Proof",
          "request": {
            "method": "POST",
            "body": {"mode": "formdata", "formdata": [{"key": "address_proof", "type": "file", "src": ""}]},
            "url": "{{base_url}}/kyc/verification/address-proof/"
          }
        },
        {
          "name": "Submit Bank Details",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"account_holder_name\": \"John Doe\",\n    \"account_number\": \"123456789012\",\n    \"ifsc_code\": \"SBIN0001234\",\n    \"bank_name\": \"State Bank of India\"\n}"},
            "url": "{{base_url}}/kyc/bank/"
          }
        },
        {
          "name": "Submit KYC for Review",
          "request": {"method": "POST", "url": "{{base_url}}/kyc/submit/"}
        },
        {
          "name": "Send OTP (KYC)",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"otp_type\": \"EMAIL\",\n    \"email\": \"john.doe@example.com\"\n}"},
            "url": "{{base_url}}/auth-user/send-otp/"
          }
        },
        {
          "name": "Verify OTP (KYC)",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"otp_type\": \"EMAIL\",\n    \"otp_code\": \"123456\"\n}"},
            "url": "{{base_url}}/auth-user/verify-otp/"
          }
        }
      ]
    },
    {
      "name": "06. KYC Admin (Admin Only)",
