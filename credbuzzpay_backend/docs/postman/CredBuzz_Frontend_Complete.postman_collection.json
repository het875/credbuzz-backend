{
  "info": {
    "name": "CredBuzz Frontend Integration - Complete API",
    "description": "Complete API collection for frontend developers. Organized by flow: Registration → Login → KYC → Dashboard → User Management → Bill Pay",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {"key": "base_url", "value": "http://127.0.0.1:8000/api"},
    {"key": "access_token", "value": ""},
    {"key": "refresh_token", "value": ""},
    {"key": "user_id", "value": "1"}
  ],
  "auth": {
    "type": "bearer",
    "bearer": [{"key": "token", "value": "{{access_token}}", "type": "string"}]
  },
  "item": [
    {
      "name": "01. Health Check",
      "item": [
        {
          "name": "Health Check",
          "request": {"method": "GET", "url": "{{base_url}}/health/", "auth": {"type": "noauth"}},
          "description": "Check if API is healthy. No auth required."
        },
        {
          "name": "Detailed Health Check",
          "request": {"method": "GET", "url": "{{base_url}}/health/detailed/", "auth": {"type": "noauth"}}
        }
      ]
    },
    {
      "name": "02. User Registration & OTP",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"first_name\": \"John\",\n    \"middle_name\": \"\",\n    \"last_name\": \"Doe\",\n    \"email\": \"john.doe@example.com\",\n    \"phone_number\": \"9876543210\",\n    \"password\": \"SecurePass@123\",\n    \"confirm_password\": \"SecurePass@123\"\n}"},
            "url": "{{base_url}}/auth-user/register/",
            "auth": {"type": "noauth"}
          },
          "description": "Register new user. OTP sent to email automatically."
        },
        {
          "name": "Verify Registration OTP",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"email\": \"john.doe@example.com\",\n    \"otp_code\": \"123456\",\n    \"otp_type\": \"EMAIL\"\n}"},
            "url": "{{base_url}}/auth-user/verify-registration-otp/",
            "auth": {"type": "noauth"}
          }
        },
        {
          "name": "Resend Registration OTP",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"email\": \"john.doe@example.com\",\n    \"otp_type\": \"EMAIL\"\n}"},
            "url": "{{base_url}}/auth-user/resend-registration-otp/",
            "auth": {"type": "noauth"}
          }
        }
      ]
    },
    {
      "name": "03. Login & Authentication",
      "item": [
        {
          "name": "Login",
          "event": [{"listen": "test", "script": {"exec": ["var jsonData = pm.response.json();", "if (jsonData.data && jsonData.data.tokens) {", "    pm.collectionVariables.set('access_token', jsonData.data.tokens.access_token);", "    pm.collectionVariables.set('refresh_token', jsonData.data.tokens.refresh_token);", "    console.log('Tokens saved!');", "}"], "type": "text/javascript"}}],
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"identifier\": \"john.doe@example.com\",\n    \"password\": \"SecurePass@123\"\n}"},
            "url": "{{base_url}}/auth-user/login/",
            "auth": {"type": "noauth"}
          },
          "description": "Login with email/phone. Response includes tokens, KYC status, and access permissions."
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"refresh_token\": \"{{refresh_token}}\"\n}"},
            "url": "{{base_url}}/auth-user/refresh-token/",
            "auth": {"type": "noauth"}
          }
        },
        {
          "name": "Logout",
          "request": {"method": "POST", "url": "{{base_url}}/auth-user/logout/"}
        },
        {
          "name": "Forgot Password",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"email\": \"john.doe@example.com\"\n}"},
            "url": "{{base_url}}/auth-user/forgot-password/",
            "auth": {"type": "noauth"}
          }
        },
        {
          "name": "Reset Password",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"email\": \"john.doe@example.com\",\n    \"otp\": \"123456\",\n    \"new_password\": \"NewSecurePass@123\",\n    \"confirm_password\": \"NewSecurePass@123\"\n}"},
            "url": "{{base_url}}/auth-user/reset-password/",
            "auth": {"type": "noauth"}
          }
        },
        {
          "name": "Change Password",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"current_password\": \"SecurePass@123\",\n    \"new_password\": \"NewSecurePass@123\",\n    \"confirm_password\": \"NewSecurePass@123\"\n}"},
            "url": "{{base_url}}/auth-user/change-password/"
          }
        }
      ]
    },
    {
      "name": "04. User Profile & Dashboard",
      "item": [
        {
          "name": "Get My Profile",
          "request": {"method": "GET", "url": "{{base_url}}/auth-user/profile/"}
        },
        {
          "name": "Get Profile with Full Access Info",
          "request": {"method": "GET", "url": "{{base_url}}/auth-user/profile-full/"},
          "description": "Returns profile with app_access and feature_access arrays"
        },
        {
          "name": "Update My Profile",
          "request": {
            "method": "PUT",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"first_name\": \"John\",\n    \"last_name\": \"Updated\"\n}"},
            "url": "{{base_url}}/auth-user/profile/"
          }
        },
        {
          "name": "Get My Activity Logs",
          "request": {"method": "GET", "url": "{{base_url}}/auth-user/my-activity/"}
        },
        {
          "name": "Get My Permissions",
          "request": {"method": "GET", "url": "{{base_url}}/rbac/my-permissions/"},
          "description": "Returns all apps and features you have access to"
        },
        {
          "name": "Check Permission",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {"mode": "raw", "raw": "{\n    \"app_code\": \"BILL_PAY\",\n    \"feature_code\": \"PAY_BILLS\"\n}"},
            "url": "{{base_url}}/rbac/check-permission/"
          }
        }
      ]
    },
    {
      "name": "05. KYC Process",
      "item": [
        {
          "name": "Get KYC Status",
